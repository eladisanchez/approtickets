{
  "name": "eladisanchez/approtickets",
  "description": "Common package for Appro Tickets",
  "type": "library",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0",
    "barryvdh/laravel-dompdf": "^2.2",
    "filament/filament": "^3.2",
    "intervention/image": "^3.6",
    "intervention/image-laravel": "^1.2",
    "redsys/tpv": "^2.3",
    "simplesoftwareio/simple-qrcode": "^4.2",
    "shanmuga/laravel-entrust": "^5.0",
    "laravel/sanctum": "^4.0",
    "inertiajs/inertia-laravel": "^1.3",
    "filament/spatie-laravel-translatable-plugin": "^3.2",
    "spatie/icalendar-generator": "^2.8",
    "mcamara/laravel-localization": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ApproTickets\\": "src/",
      "ApproTickets\\Tests\\": "tests",
      "Database\\Seeders\\": "src/database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ApproTickets\\ApproTicketsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Eladi SÃ¡nchez",
      "email": "eladisanchez@gmail.com"
    }
  ],
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.13",
    "orchestra/testbench": "^9.1",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.4",
    "phpunit/phpunit": "^10.5",
    "nunomaduro/collision": "^8.4",
    "pestphp/pest-plugin-livewire": "^2.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Workbench\\App\\": "workbench/app/",
      "Workbench\\Database\\Factories\\": "workbench/database/factories/",
      "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@clear",
      "@prepare"
    ],
    "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
    "prepare": "@php vendor/bin/testbench package:discover --ansi",
    "build": "@php vendor/bin/testbench workbench:build --ansi",
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "@build",
      "@php vendor/bin/testbench serve"
    ]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
